<mapper namespace="sist.com.bean.apple">

	<select id="selectEmp" resultType="emp">	    
		SELECT EMPNO en,ENAME irum,SAL,NVL(COMM,0)Comm,MGR,
		TO_CHAR(HIREDATE,'YYY/MM/DD')HIREDATE
		FROM EMP
	</select>
</mapper>





